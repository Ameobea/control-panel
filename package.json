{
  "name": "react-control-panel",
  "version": "0.2.3",
  "description": "react-compatible, embeddable panel of inputs for parameter settings",
  "main": "dist/index.js",
  "module": "./lib/index.js",
  "scripts": {
    "start": "cp demo/index.html dist/index.html && webpack-dev-server",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint ./index.js ./src",
    "prepublishOnly": "rm dist/* && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameobea/react-control-panel.git"
  },
  "keywords": [
    "slider",
    "input",
    "range",
    "checkbox",
    "component",
    "color-picker",
    "dom",
    "css",
    "react"
  ],
  "author": "freeman-lab, Casey Primozic <me@ameo.link> (React port)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ameobea/react-control-panel/issues"
  },
  "homepage": "https://github.com/ameobea/react-control-panel#readme",
  "dependencies": {
    "is-numeric": "0.0.5",
    "is-string": "^1.0.4",
    "prop-types": "^15.6.2",
    "react-css-component": "^1.0.2",
    "simple-color-picker": "^0.1.2",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "standard": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
